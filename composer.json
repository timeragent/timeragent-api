{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.2",
        "folklore/graphql": "~1.0.0",
        "illuminate/redis": "^5.6",
        "laravel/lumen-framework": "5.6.*",
        "laravel/passport": "^5.0",
        "predis/predis": "~1.0",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0",
        "vluzrmos/tinker": "^1.3",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "generate-schemadoc": [
            "NODE_TLS_REJECT_UNAUTHORIZED=0 graphdoc -e https://timeragent-api.test/graphql -o ./public/schema -f",
            "open https://timeragent-api.test/schema"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
